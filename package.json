{
  "name": "test-api",
  "version": "0.0.1-SNAPSHOT",
  "description": "Api Test",
  "main": "index.js",
  "nodemonConfig": {
    "ignore": [
      "public/**/public",
      "test/**/*.js",
      "seeds/**/*.js",
      "node_modules/**/node_modules",
      "public/lote/*.*",
      "public/lote/*.csv",
      "public/lote/*.DBF",
      "public/lote/*.txt",
      "public/lote/*.TXT",
      "public/lote/*.CSV",
      "public/jsonFiles/*.json",
      "public/lotesPreparados/*.json"
    ],
    "restartable": "rs",
    "delay": "3500",
    "env": {
      "NODE_ENV": "development"
    }
  },
  "scripts": {
    "start:migrate": "npm run db:migrate && node index.js",
    "start": "node index.js",
    "dev": "gulp dev",
    "test": "gulp eslint && npm run test:cover",
    "test:teamcity": "cross-env NODE_ENV=test nyc mocha",
    "mocha": "cross-env NODE_ENV=test mocha",
    "test:cover": "cross-env NODE_ENV=test nyc mocha",
    "lint": "gulp eslint",
    "build": "gulp build",
    "db:migrate": "npx knex migrate:latest",
    "db:seed": "npx knex seed:run",
    "db:rollback": "knex migrate:rollback"
  },
  "directories": {
    "test": "./test"
  },
  "nyc": {
    "all": true,
    "reporter": [
      "text",
      "html",
      "teamcity"
    ],
    "check-coverage": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "exclude": [
      "coverage/**/*.js",
      "commitlint.config.js",
      "gulpfile.js",
      "knexfile.js",
      "src/app.js",
      "src/global.js",
      "src/index.js",
      "db/**/*.js",
      "test/**/*.js",
      "src/services/arqService.js"
    ]
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cron": "^1.8.1",
    "csvtojson": "^2.0.10",
    "dbffile": "^1.5.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-openapi-validator": "^3.6.6",
    "helmet": "^3.21.2",
    "jsonfile": "^6.1.0",
    "knex": "^0.20.8",
    "lodash": "^4.17.20",
    "moment": "^2.24.0",
    "mongoose": "^5.8.11",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "node-fetch": "^2.6.1",
    "pg": "^7.18.1",
    "request": "^2.88.0",
    "require-dir": "^1.2.0",
    "saslprep": "^1.0.3",
    "sqlite3": "^4.1.1",
    "swagger-ui-express": "^4.1.3",
    "uuidv4": "^6.0.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "babel-eslint": "^10.0.3",
    "cross-env": "^6.0.3",
    "del": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-lodash": "^6.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "expect": "^24.9.0",
    "gulp": "^4.0.2",
    "gulp-eslint": "^6.0.0",
    "gulp-json-editor": "^2.5.4",
    "gulp-load-plugins": "^2.0.2",
    "gulp-nodemon": "^2.5.0",
    "gulp-rename": "^2.0.0",
    "husky": "^4.2.0",
    "mocha": "^7.0.0",
    "mock-knex": "^0.4.7",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "should": "^13.2.3",
    "sinon": "^8.1.0",
    "supertest": "^4.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://"
  },
  "bugs": {
    "url": "https://"
  },
  "homepage": "https://",
  "engines": {
    "node": ">=12.14"
  },
  "author": "gverges@INDEC (https://github.com/indec-it)",
  "license": "MIT"
}
